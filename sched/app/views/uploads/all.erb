<h1><%=current_user.username.capitalize%>'s Uploads</h1><hr><br><br>
<%@uploads.each do |all_uploads|%>
    <%if all_uploads.user_id == current_user.id%>
        <%=all_uploads.entry%>
        <form action="/uploads/<%="#{all_uploads.id}"%>" method="post">
           <input type="hidden" name="_method" value="delete">

           <div class="event_buttons2">
           <button class="eventbutton eventbutton2" type="submit">Delete</button>
           </div>
       </form><hr><br><br>
    <%end%>  
<%end%>
